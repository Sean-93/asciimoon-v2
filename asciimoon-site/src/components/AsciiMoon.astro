<style></style>
<script is:inline>
// THIS MAKES SURE THAT THE CIRCLE IS SAME HEIGHT AS <pre> ELEMENT.  WIDTH IS TAKEN CARE OF IN "aspect-square" TAILWIMD CLASS

function setCircleSize() {
  const asciiMoon = document.getElementById("asciiMoon");
  const circleElement = document.getElementById("circleElement");

  // Fetch the computed style of the pre element
  const asciiMoonStyle = window.getComputedStyle(asciiMoon);

  // Set the height of the circle element to match the pre element
  circleElement.style.height = asciiMoonStyle.height;
}

window.onload = function () {
  setCircleSize();
};

window.addEventListener("resize", setCircleSize);
</script>
